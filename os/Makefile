include rules.mk
# --silent
V=



default: all

info:	
	@echo "----------------------------------------------------------------------"
	@make  $(V) -f do.mk info 
	@echo info ...done.
	@echo "----------------------------------------------------------------------"

depend:	
	@echo "----------------------------------------------------------------------"
	@make  $(V) -f do.mk depend 
	@echo info ...done.
	@echo "----------------------------------------------------------------------"



all:		
	@echo "----------------------------------------------------------------------"
	@make    $(V)  -f do.mk all 
	@echo all ...done.
	@echo "----------------------------------------------------------------------"


asm:		
	@echo "----------------------------------------------------------------------"
	@make    $(V)  -f do.mk asm 
	@echo all ...done.
	@echo "----------------------------------------------------------------------"


clean:	
	@echo "----------------------------------------------------------------------"
	@make   $(V) -f do.mk clean
	@echo clean ...done.
	@echo "----------------------------------------------------------------------"
	
dwload:	
	@echo "----------------------------------------------------------------------"
	@make   $(V) -f do.mk dwload
	@echo dwload ...done.
	@echo "----------------------------------------------------------------------"
	
qemu:	
	@echo "----------------------------------------------------------------------"
	@make   $(V) -f do.mk qemu
	@echo qemu ...done.
	@echo "----------------------------------------------------------------------"
		
qemu_deb:	
	@echo "----------------------------------------------------------------------"
	@make   $(V) -f do.mk qemu_deb
	@echo qemu_deb ...done.
	@echo "----------------------------------------------------------------------"
		
distclean: 
	@echo "----------------------------------------------------------------------"
	@make   $(V) -f do.mk distclean
	@echo distclean ...done
	@echo "----------------------------------------------------------------------"

.PHONY: clean dwload qemu all raspi2 distclean error

